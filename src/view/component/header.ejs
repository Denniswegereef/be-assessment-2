<header>
    <a href="/">Movie dates</a>
    <ul>
        <% if (sessionUser) { %>
        <li>
            <a href="../account">Hello <%= sessionUser.user %></a>
        </li>
        <li>
            <% if (sessionUser.info.image){ %>
            <img src="images/users/<%= sessionUser.info.image %>" alt="">
            <% } else { %>
            <img src="../images/users/profile-placeholder.jpg" alt="">
            <% } %>
        </li>
        <li>
            <a href=../log-out>Log out</a>
        </li>
        <% } else { %>
        <li>
            <a href="../login">Log-in</a>
        </li>
        <li>
            <a href="../register">Register</a>
        </li>
        <% } %>
    </ul>
</header>
